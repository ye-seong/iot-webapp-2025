@model IEnumerable<DbFirstWebApp.Models.Rentaltbl>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RentalDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReturnDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookIdxNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemberIdxNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RentalDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReturnDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BookIdxNavigation.Idx)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemberIdxNavigation.Idx)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idx">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idx">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idx">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
