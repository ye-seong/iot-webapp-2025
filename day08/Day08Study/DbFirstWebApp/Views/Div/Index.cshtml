@model IEnumerable<DbFirstWebApp.Models.Divtbl>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-sm btn-primary">Create</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Division)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Names)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Division)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Names)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.Division" class="btn btn-sm btn-secondary">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Division" class="btn btn-sm btn-success">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Division" class="btn btn-sm btn-danger">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
